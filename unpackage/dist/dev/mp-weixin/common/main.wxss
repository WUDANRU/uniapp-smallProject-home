.w{
	width:750rpx;
}
.h{
	height:100rpx;
}
.font-w{
	font-weight: 400;
}
.font-bold{
	font-weight: bold;
}
.font-1a{
	font-size:10rpx;
}
.font-1b{
	font-size:15rpx;
}
.font-2{
	font-size:25rpx;
}
.font-2a{
	font-size:28rpx;
}
.font-3{
	font-size:30rpx;
}
.font-4a{
	font-size:43rpx;
}
.font-4{
	font-size:45rpx;
}
.font-5{
	font-size:50rpx;
}
.letter-s-1{
	letter-spacing:-10rpx;
}
.letter-s-2{
	letter-spacing:-4rpx;
}
/* border */
.border{
	border:1rpx solid #edf1ee;
}
.border-2{
	border:1rpx solid #d8b190;
}
.border-3{
	border:1rpx solid #494c51;
}
.border-4{
	border:1rpx solid #eeeeee;
}
.border-5{
	border:1rpx solid #e7ebee;
}
.border-6{
	border:1px solid #d6e1e1;
}
.border-7{
	border:1px solid #23c993;
}
/* position */
.p-fixed{
	position:fixed;
}
/* flex */
.d-flex{
	display:flex;
}
.flex-wrap{
	flex-wrap: wrap;
}
.d-inline-flex{
display: inline-flex;
}
.d-none{
	display:none;
}
.j-center{
	justify-content:center;
}
.j-around{
	justify-content:space-around;
}
.j-sb{
	justify-content: space-between;
}
.f-r{
	flex-direction: row;
}
.f-c{
	flex-direction: column;
}
.align-c{
	align-items: center;
}
